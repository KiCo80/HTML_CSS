a{
    color:blue;
}
a:active{
    color: black;
}
a:visited{
    color: purple;
}
a:hover{
    color: red;
}
ul{
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
}
li{
    float: left;
    background-color: greenyellow;
}
li a{
    display: block;
    padding: 8px;
}
li a:hover{
    background-color: rgb(87, 81, 81);
    color: magenta;
}
